{
    "name": "tylercd100/monolog-plivo",
    "description": "A Monolog Handler for Plivo, a SMS messaging service.",
    "keywords": [
        "monolog",
        "plivo",
        "handler",
        "composer",
        "tylercd100"
    ],
    "homepage": "https://github.com/tylercd100/monolog-plivo",
    "license": "MIT",
    "autoload":{
        "psr-4":{
            "Tylercd100\\Monolog\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tylercd100\\Monolog\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^5.4|^7.0",
        "monolog/monolog": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "authors": [
        {
            "name": "Tyler Arbon",
            "email": "tylercd100@gmail.com"
        }
    ]
}
